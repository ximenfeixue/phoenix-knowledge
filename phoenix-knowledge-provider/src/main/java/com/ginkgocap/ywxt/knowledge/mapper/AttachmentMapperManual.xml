<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ginkgocap.ywxt.knowledge.mapper.AttachmentMapperManual">
	<insert id="insertSelective" useGeneratedKeys="true"
		keyProperty="id" parameterType="com.ginkgocap.ywxt.knowledge.entity.Attachment">
		insert into
		tb_attachment(taskId,file_name,file_size,file_type,download_url,userId)
		values ( #{taskId}, #{fileName}, #{fileSize}, #{fileType},
		#{downloadUrl}, #{userid} )
	</insert>
</mapper>